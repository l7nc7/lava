}

dstat1()
{
    clear
    echo -e " ${CYAN}
  ┌───────────────────────────────────────────────────────────────────────┐
  │                        \033[1;91mDSTAT${CYAN}                                          │
  └───────────────────────────────────────────────────────────────────────┘
"
dstat
}

