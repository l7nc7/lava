Take_input1()
{
clear
while :
do
clear;clear
echo -en "

                   ·▄▄▄▄  ▄▄▄ .• ▌ ▄ ·.        ▐ ▄ 
                   ██· ██ ▀▄.▀··██ ▐███▪ ▄█▀▄ •█▌▐█
                   ▐█▪ ▐█▌▐▀▀▪▄▐█ ▌▐▌▐█·▐█▌.▐▌▐█▐▐▌
                   ██. ██ ▐█▄▄▌██ ██▌▐█▌▐█▌.▐▌██▐█▌
                   ▀▀▀▀▀•  ▀▀▀ ▀▀  █▪▀▀▀ ▀█▄▀▪▀▀ █▪
                 \033[1;91m罪 深 い PRIVATE Panel\033[00m

\033[1;91m┌──────────────────────────────────┐
\033[1;91m│${CYAN} ======UDP======
\033[1;91m│${CYAN} [1] UDP-ENGINE[5-9GBPS]
\033[1;91m│${CYAN} [2] FORTNITE-Lagg[original port]
\033[1;91m│${CYAN} [3] PRIV UDP[2-4GBPS]
\033[1;91m│${CYAN} [4] FBI UDP[1-6MBPS]
\033[1;91m│${CYAN} [5] LDAP[1-5MBPS]
\033[1;91m│${CYAN} ======TCP&BYPASS======
\033[1;91m│${CYAN} [6] xSYN
\033[1;91m│${CYAN} [7] XSSHEX
\033[1;91m│${CYAN} [8] PRIVATE BYPASS[HighPPS]
\033[1;91m│${CYAN} [9] NFO BYPASS
\033[1;91m│${CYAN} ======LAYER7======
\033[1;91m│${CYAN} [10] HTTP FLOOD
\033[1;91m│${CYAN} [11] HTTPS FLOOD
\033[1;91m│${CYAN} ======MISC======
\033[1;91m│${CYAN} [12] BYPASS HUB
\033[1;91m│${CYAN} [13] SNAKE GAME
\033[1;91m│${CYAN} [14] SPEEDTEST
\033[1;91m│${CYAN} [15] DSTAT 1
\033[1;91m│${CYAN} [16] DSTAT 2
\033[1;91m│${CYAN} [17] USERS
\033[1;91m│${CYAN} [CTRL+C] TO STOP THE ATTACKS.
\033[1;91m│${CYAN} [C] CREDITS.
\033[1;91m│${CYAN} [X] CLOSE.
\033[1;91m└──────────────────────────────────┘
${CYAN}[\033[1;91mBy I7nc7${CYAN}]-家 "
    read yourch
    case $yourch in
      1) udp_flooda ;;
      2) udp_floodb ;;
	  3) x8g7 ;;
	  4) fbi ;;
	  5) ldap ;;
	  6) nfobypass ;;
	  7) xsshex ;;
	  8) privbypass ;;
	  9) nfobypass ;;
	  10) http ;;
	  11) https ;;
	  12) cortex ; exit 1 ;;
	  13) snake ;;
	  14) speedtest ;;
	  15) vnstat -l -i eth0 ;;
	  16) dstat1 ;;
	  17) users ;;
	  C) credits ; exit 1 ;;
	  X) exit 1  ;;
      *) echo "INVALID SELECTION," ;
         echo "PRESS ENTER." ; read ;;
 esac
done # end_while
}
#
# Set trap to for CTRL+C interrupt i.e. Install our error handler
# When occurs it first it calls del_file() and then exit
#
trap exit_script 2
#
# Call our user define function : Take_inpt1
#

Take_input1